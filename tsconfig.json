{
  "compilerOptions": {
    "outDir": "./dist/",
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "skipLibCheck": true,
    "skipDefaultLibCheck": true
  }
}
